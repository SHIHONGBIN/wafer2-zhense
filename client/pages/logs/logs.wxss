.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
.pageCon{ background-color: #FaFFFc;}
.scroll-view{width:100%; }
.footerNav{height: 50px; border-top: solid 1px #e0e0e0; position: fixed; left: 0; bottom: 0;width:100%;text-align: center; line-height: 50px;z-index: 9997}
.footerNav .footerbox{ display: flex; flex-direction: row; background-color: #f5f5f5;}
.footerNav .l-nav{flex: 1}
.footerNav .r-nav{flex: 1;border-left: solid 1px #e0e0e0;}
.itembox{height: 12vw; border-bottom: solid 1px #f5f5f5; line-height: 12vw; font-size: 0; overflow: hidden; zoom:1;width: 100vw;}
.itembox .num,.itembox .img1,.itembox .img2,.itembox .text{ display: inline-block; font-size: 14px;vertical-align: middle}
.itembox .img1,.itembox .img2{width:10vw; height: 10vw; margin: 1vw; border-radius: 50%; box-shadow: 1px 1px 2px rgba(0,0,0,.2); border: solid 1px #f5f5f5;}
.itembox .text{line-height: normal;width: 69vw; overflow: hidden; font-size: 0}
.itemBox-wrap1 .itembox .text{width: 53vw; }
.itembox1 .text .text-sub{ display: inline-block; height: 6vw; line-height: 6vw;font-size: 14px;}
.itembox .text .text-sub{ display: inline-block; height: 6vw; line-height: 6vw;width: 45%;padding-left: 5%;font-size: 4vw;} 
.itembox .num{width:5vw;text-align: center;}
.itemBox-wrap1 .itembox:last-child{ border: none}
.itemBox-wrap1 .text .text1,.itemBox-wrap1 .text .text2{ display: block; width:95%;}
.itemBox-wrap .itembox:nth-child(odd){ background-color: #f5f5f5;}
.pageCon1 .killsort,.pageCon1 .imgbox,.pageCon1 .tips,.pageCon1 .num{width:100%; min-height:12.5vw; font-size: 4vw;padding:0}
.pageCon1{width:100%;}
.pageCon1 .itembox{ height: auto; line-height: normal; font-size: 14px; vertical-align: middle; border:none;}
.pageCon1 .itembox .itembox-con{ height: 12.5vw; position: relative}
.pageCon1 .itembox .num{ display: block;width:100%; color:#fff; text-align: center; height: auto; color:#74b9ca; }
.pageCon1 .itembox .imgbox{width: 100%;vertical-align: middle; padding-top: 1vw;}
.pageCon1 .itembox .imgbox image{width:11vw; height: 11vw; margin: 0.25vw; display:block; float: left;border-radius: 50%;box-shadow: 1px 1px 2px rgba(0,0,0,.2); border: solid 1px #f5f5f5;}
.pageCon1 .itembox .imgbox image:nth-child(2){margin-left: 13.5vw;}
.pageCon1 .itembox .imgtips{ display: inline-block;width: 10.5vw;vertical-align: middle;float: left; text-align: center; padding: 0 1vw;font-size: 14px; height: 11.5vw;padding-top:2vw;}
.pageCon1 .itembox .killsort-con{ width:85.5vw;display: inline-block; float: right; padding: 0 1vw; font-size: 14px; padding-top: 1vw;}
.pageCon1 .num{line-height: normal;height:24px; line-height: 24px; text-align: left;min-height:auto; background-color: rgba(155,213,234,.2); font-size: 14px; box-shadow: 2px 2px 2px rgba(155,213,234,.5); z-index: 9}
.pageCon1 .killsort,.pageCon1 .tips{height: auto; min-height: auto; overflow: visible}
.pageCon1 .itembox .itembox-con:nth-child(even){ background-color:#f5f5f5}
.pageCon1 .itembox .itembox-con .imgtips{ display: block; color:rgba(70, 130, 180,1);border-top-right-radius: 20%; position: absolute; left: 0; top: -1vw; z-index: 99;}
.pageCon1 .itembox .itembox-con:nth-child(3) .imgtips{background-color: rgba(247, 251, 255,1);box-shadow: 2px 0 2px rgba(0,0,0,.2)}
.pageCon1 .itembox .itembox-con:nth-child(3) {background-color: rgba(240, 248, 255,.3);}
.pageCon1 .itembox .itembox-con:nth-child(5) .imgtips{background-color: rgba(245, 245, 245,1); box-shadow: 2px 0 2px rgba(0,0,0,.2)}
.pageCon1 .itembox .itembox-con:nth-child(5) {background-color: rgba(229, 229, 229,.3);}
.pageCon1 .itembox .itembox-con:nth-child(7) .imgtips{background-color: rgba(253, 250, 243,1);box-shadow: 2px 0 2px rgba(0,0,0,.2)}
.pageCon1 .itembox .itembox-con:nth-child(7) {background-color: rgba(253, 245, 230,.3);}
.pageCon1 .itembox .itembox-con:nth-child(4){border: none;}
.clear{ clear:both}
.curHover{ background-color:rgba(155,213,234,1); color:#fff;}
.text{ display: inline-block;vertical-align: middle;}
.text-icon{height:0; width: 0; border: solid 5px rgba(0,0,0,0); border-left: solid 8px #74b9ca; display: inline-block;vertical-align: middle; animation: iconanimate 2s infinite .5s;}
@keyframes iconanimate{
  0% {transform:  rotateX(0) ; perspective: 500px;}
  10% {transform:  rotateX(0) ; perspective: 500px;}
  90% {transform:  rotateX(360deg) ; perspective: 500px;}
  100% {transform:rotateX(360deg); perspective: 500px; }
}
.title{text-align: left;vertical-align: middle;position: relative; overflow: hidden;color:#74b9ca; border-bottom: solid 1px rgba(155,213,234,.6);padding: 5px 10px;  font-size: 14px; background: rgba(155,213,234,.2)}
.text{ display: inline-block;vertical-align: middle;}
.text-icon{height:0; width: 0; border: solid 4px rgba(0,0,0,0); border-left: solid 8px #74b9ca; display: inline-block;vertical-align: middle; animation: iconanimate 2s infinite .5s;}
@keyframes iconanimate{
  0% {transform:  rotateX(0) ; perspective: 500px;}
  10% {transform:  rotateX(0) ; perspective: 500px;}
  90% {transform:  rotateX(360deg) ; perspective: 500px;}
  100% {transform:rotateX(360deg); perspective: 500px; }
}
.text-icon2{
   border-left: solid 4px rgba(0,0,0,0);border-right: solid 8px #74b9ca; 
}
.messagesComponent{width: 100%; padding-bottom: 50px;}
.iphonexcss .l-nav,.iphonexcss .r-nav,.iphonexcss,.iphonexcss .footerbox{padding-bottom: 20px;}
.messagesComponent.iphonexcss{padding-bottom:70px;}
